{{> breadcrumbs this }}

<div class="page-header p-3">
	<div class="row">
		<div class="col-12">
			<h1 class="text-truncated">{{page_title}}</h1>
		</div>
	</div>
	<hr>
</div>

<div id="mainbody" class="m-2">
	<table class="table table-hover datatable" cellspacing="0" width="100%">
	    <thead>
	        <tr class="palette-bg-teal-500 text-white">
	            <th>App</th>
	            <th>Scope</th>
	            <th>E-Mail</th>
	            <th>Subject</th>
	            <th>Comment</th>
	            <th>Satisfaction</th>
	            <th>&nbsp;</th>
	        </tr>
	    </thead>
	    <tbody>	
		    {{#feedback}}
				<tr>
					<th>{{appTitle}}</th>
		            <th>{{scope}}</th>
		            <th>{{email}}</th>
		            <th>{{subject}}</th>
		            <th>{{comment}}</th>
		            <th>{{satisfaction}}</th>
		            <th>&nbsp;</th>
				</tr>
			{{/feedback}}
	    </tbody>
	</table>
</div>