<h2><i class="fa fa-github"></i> Latest Changes</h2>

<ul class="list-group">
	{{#commits}}
  		<li class="list-group-item pl-1 pr-1">
  			<p class="font-weight-bold text-truncate">
  				<a href="{{html_url}}" class="text-capitalize">{{commit.message}}</a> <span class="float-xs-right">{{commit.committer.date}}</span></p>
  			<div class="chip">
			  	<img src="{{committer.avatar_url}}" alt="Person" width="96" height="96">
			  	{{commit.committer.name}}
			</div>

  		</li>
  	{{/commits}}
</ul>